class Solution {
    public int numJewelsInStones(String J, String S) {
        int count = 0;
        int i,j;
        for(i = 0 ; i<J.length() ; i++)
        {
            for(j = 0;j<S.length();j++) {
                if (S.charAt(j) == J.charAt(i)) {
                    count++;
                }
            }
        }
        return  count;
    }
}
